var new_feed = '<%= scheduler_feed.html_safe %>';

// remove the old calendar feed
$('#calendar').fullCalendar('removeEventSource', '<%= params[:old_feed].html_safe %>');

// give the calendar a new feed
$('#calendar').fullCalendar('addEventSource', new_feed);

// update the form with the new feed
$('#old_feed').val(new_feed);

// additionally, need to change the autocomplete source so that it
// looks for course instances in the correct day range
$('#course_title').autocomplete('option', 'source', '<%= course_instance_autocomplete(@search_date || Date.today).html_safe %>');
