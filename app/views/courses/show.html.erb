<div class="no-gutter row">
  <%= render 'sidebar' %>
    
  <!--mid column-->
  <div class="col-md-3">

    <div class="panel" id="midCol">
    <div class="panel-heading" style="background-color:#555;color:#eee;"></div> 
    <div class="panel-body">
      <div class="well">
          <h3><%=first_professor.name%></h3>
      </div>
      
      <hr>
      
      <h3>Previous Semesters</h3>
      
      <% @instances_by_semester.each do |semester, instances| %>
        <p><b><%= semester %></b>:</p>
        <% instances.each do |instance| %>
          Section <%= instance.section %> taught by <%= instance.professor.name %><br />
          <%= instance.meetings.first.schedule %><br />
        <% end %>
      <% end %>
      
     </div> 
     </div><!--/panel-->
  </div><!--/end mid column-->
  
  <!-- right content column-->
  <div class="col-md-7" id="content">
    <div class="panel">
      <div class="panel-heading" style="background-color:#111;color:#fff;"></div>
      
      <div class="panel-body">
        <div class="row">
          <div class="col-md-8">
          <h2><%= @course.long_string %></h2>
          <table border="0" id="ACE_SSR_CLS_DTL_WRK_GROUP6" cellpadding="0" cellspacing="0" cols="2" width="541">
            <tbody>
              <tr>
                <td valign="top" align="left">
                  <h4>Description:</span></h4>
                  <p><%= @course.description %></p>
                  <p>&nbsp;</p>
                  <h4>Prereqs:</h4>
                  <p>
                  <ul>
                    <% prereq_sets(@course).each do |prereq_set| %>
                      <%= prereq_set.html_safe %>
                    <% end %>
                  </ul>
                  </p>
                  <h4>Required for:</h4>
                  <p>TODO</p>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
      
        <hr>
                  
        <h2>Tips and Reviews</h2>
        <p>TODO: Populate this via the Posts model</p>
        
        <hr>
      
        </div><!--/panel-body-->
      </div><!--/panel-->
    <!--/end right column-->
  </div> 
</div>
